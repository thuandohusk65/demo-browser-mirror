<?xml version="1.0" encoding="utf-8"?><com.google.android.gms.ads.formats.UnifiedNativeAdView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/native_ad_view"    android:layout_width="match_parent"    android:layout_height="450dp"    android:background="@drawable/black_background">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/background"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_centerInParent="true"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/headline"            android:layout_width="match_parent"            android:layout_height="50dp"            android:layout_weight="0.5"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <ImageView                android:id="@+id/icon"                android:layout_width="48dp"                android:layout_height="48dp"                android:layout_marginStart="5dp"                android:layout_marginTop="5dp"                android:layout_marginBottom="5dp"                android:scaleType="fitCenter"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent"                tools:background="@drawable/black_background" />            <TextView                android:id="@+id/ad_notification_view"                android:layout_width="@dimen/gnt_ad_indicator_width"                android:layout_height="@dimen/gnt_ad_indicator_height"                android:layout_marginStart="8dp"                android:layout_marginTop="4dp"                android:background="@drawable/admob_rounded_corners_shape"                android:gravity="center"                android:text="Ad"                android:textColor="@color/white"                android:textSize="@dimen/gnt_ad_indicator_text_size"                android:textStyle="bold"                app:layout_constraintStart_toEndOf="@+id/icon"                app:layout_constraintTop_toTopOf="parent" />            <TextView                android:id="@+id/sponsoredText"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="5dp"                android:padding="1dp"                android:text="sponsored"                android:textColor="#666"                android:textSize="11sp"                android:textStyle="bold"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toEndOf="@+id/icon"                app:layout_constraintTop_toBottomOf="@+id/ad_notification_view" />            <TextView                android:id="@+id/primary"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_marginStart="5dp"                android:layout_marginTop="3dp"                android:layout_marginEnd="5dp"                android:lines="1"                android:textColor="#fefefe"                android:textSize="@dimen/gnt_text_size_small"                android:textStyle="bold"                app:layout_constraintBottom_toTopOf="@+id/rating_bar"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toEndOf="@+id/sponsoredText"                app:layout_constraintTop_toTopOf="parent" />            <RatingBar                android:id="@+id/rating_bar"                style="?android:attr/ratingBarStyleSmall"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="8dp"                android:layout_marginBottom="3dp"                android:lines="1"                android:numStars="0"                android:textColor="@color/gnt_gray"                android:textSize="10sp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toEndOf="@+id/sponsoredText">            </RatingBar>        </androidx.constraintlayout.widget.ConstraintLayout>        <com.google.android.gms.ads.formats.MediaView            android:id="@+id/media_view"            android:layout_width="match_parent"            android:layout_height="270dp"            android:layout_margin="@dimen/gnt_no_size"            android:layout_marginTop="8dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/headline" />        <androidx.constraintlayout.widget.ConstraintLayout            android:layout_width="match_parent"            android:layout_height="130dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/media_view">            <androidx.appcompat.widget.LinearLayoutCompat                android:id="@+id/linearLayoutCompat"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginStart="8dp"                android:layout_marginEnd="8dp"                android:orientation="horizontal"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <TextView                    android:id="@+id/secondary"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:gravity="top"                    android:lines="1"                    android:textColor="#fefefe"                    android:textSize="@dimen/gnt_ad_indicator_text_size"                    android:textStyle="bold"                    tools:text="vvvv vvvv" />                <TextView                    android:id="@+id/ad_advertiser"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:gravity="top"                    android:lines="1"                    android:textColor="#fefefe"                    android:textSize="@dimen/gnt_ad_indicator_text_size"                    android:textStyle="bold"                    tools:text="vvv vvv" />            </androidx.appcompat.widget.LinearLayoutCompat>            <TextView                android:id="@+id/body"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_margin="@dimen/gnt_no_size"                android:layout_marginStart="8dp"                android:layout_marginEnd="8dp"                android:ellipsize="end"                android:maxLines="3"                android:paddingStart="@dimen/gnt_default_margin"                android:paddingBottom="@dimen/gnt_default_margin"                android:textColor="#fefefe"                android:textSize="14sp"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat" />            <Button                android:id="@+id/cta"                android:layout_width="0dp"                android:layout_height="42dp"                android:layout_marginStart="24dp"                android:layout_marginEnd="24dp"                android:layout_marginBottom="8dp"                android:background="@drawable/admob_cta_corners_shape"                android:gravity="center"                android:lines="1"                android:textColor="@color/gnt_white"                android:textSize="18sp"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent"                tools:text="Download" />        </androidx.constraintlayout.widget.ConstraintLayout>    </androidx.constraintlayout.widget.ConstraintLayout></com.google.android.gms.ads.formats.UnifiedNativeAdView>